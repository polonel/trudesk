  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-standard",
        "git add"
      ]
    },
    "ignore": [
      "public/js/*.js",
      "src/public/js/vendor/**/*.min.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard": {
    "ignore": [
      "/public/",
      "/mobile/",
      "/src/public/js/plugins/",
      "/src/public/js/vendor/"
    ],
    "globals": [
      "define",
      "MG",
      "Snackbar",
      "ROLES",
      "io",
      "d3",
      "History"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "bracketSpacing": true,
    "endOfLine": "lf"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": [
        "node_modules",
        "tests"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "snyk": true,
  "packageManager": "yarn@3.2.1"
}
